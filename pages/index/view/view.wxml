      <swiper-item>
        <view> indicator-dots="true" </view>
        <view>是否显示面板指示点</view>
      </swiper-item>
      <swiper-item>
        <view>indicator-color="#0f0"</view>
        <view>指示点颜色</view>
      </swiper-item>
      <swiper-item>
        <view>indicator-active-color="#0ff"</view>
        <view>选中指示点颜色</view>
      </swiper-item>
      <swiper-item>
        <view> current="0" </view>
        <view>第一个显示的</view>
        <view>若是固定值,则为初始值</view>
        <view>若是动态,则为显示值</view>
      </swiper-item>
      <swiper-item>
        <view>autoplay="true"</view>
        <view>是否自动替换</view>
      </swiper-item>
      <swiper-item>
        <view> interval="1200"</view>
        <view>自动更改时间</view>
      </swiper-item>
      <swiper-item>
        <view> duration="600"</view>
        <view>动画时常</view>
      </swiper-item>
      <swiper-item>
        <view>easing-function="linear"</view>
        <view>动画效果:</view>
        <view>default</view>
        <view>linear</view>
        <view>easeInCubic</view>
        <view>easeOutCubic</view>
        <view>easeInOutCubic</view>
      </swiper-item>
      <swiper-item>
        <view>circular="{{true}}" </view>
        <view>是否衔接滑动</view>
      </swiper-item>
      <swiper-item>
        <view>vertical="{{false}}"</view>
        <view>是否垂直</view>
      </swiper-item>
      <swiper-item>
        <view>previous-margin="3rpx"</view>
        <view>前编剧</view>
      </swiper-item>
      <swiper-item>
        <view>next-margin="2rex"</view>
        <view>后边距</view>
      </swiper-item>
      <swiper-item>
        <view>display-multiple-items="1"</view>
        <view>同时显示滑块数量</view>
      </swiper-item>
      <swiper-item>
        <view>bindchange=""
        </view>
        <view> bindtransition=""</view>
        <view>bindanimationfinish=""</view>
      </swiper-item>
    </swiper>
