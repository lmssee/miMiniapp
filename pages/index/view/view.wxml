  <view>
    <swiper indicator-dots="true" indicator-color="#0f0" indicator-active-color="#0ff" current="0" autoplay="true"
      interval="2500" duration="2500" easing-function="linear" circular="{{true}}" vertical="{{false}}"
      previous-margin="0rpx" next-margin="2rex" display-multiple-items="3" bindchange="" bindtransition=""
